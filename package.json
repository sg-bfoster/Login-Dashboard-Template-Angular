{
  "name": "angular-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "check:pre-publish": "node scripts/check-pre-publish.js",
    "config:generate": "node scripts/generate-supabase-config.js",
    "config:generate:auth": "node scripts/generate-auth-config.js",
    "config:blank": "node scripts/write-blank-supabase-config.js && node scripts/write-blank-auth-config.js",
    "config:blank:auth": "node scripts/write-blank-auth-config.js",
    "fix:native": "node scripts/ensure-native-binaries.js",
    "postinstall": "node scripts/ensure-native-binaries.js",
    "prestart": "node scripts/ensure-native-binaries.js && node scripts/generate-supabase-config.js && node scripts/generate-auth-config.js",
    "prebuild": "node scripts/ensure-native-binaries.js && node scripts/generate-supabase-config.js && node scripts/generate-auth-config.js",
    "prewatch": "node scripts/ensure-native-binaries.js",
    "pretest": "node scripts/ensure-native-binaries.js",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.3.17",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@okta/okta-auth-js": "^7.14.1",
    "@okta/okta-signin-widget": "^7.40.3",
    "@supabase/supabase-js": "^2.47.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.52.3",
    "@rollup/rollup-darwin-x64": "^4.52.3",
    "@rollup/rollup-linux-x64-gnu": "4.52.3",
    "@rollup/rollup-win32-x64-msvc": "4.52.3"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "dotenv": "^16.4.5",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
